{
	"name": "obsidian-grafika-plugin",
	"version": "0.5.0",
	"description": "Obsidian plugin for embedding plots, charts and graphs in markdown documents, currently supporting Apache echarts, Chart.js and vis.js network and timeline",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"grafika",
		"plot",
		"chart",
		"graph",
		"vis.js",
		"chart.js",
		"observable-plot"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/d3": "^7.4.3",
		"@types/node": "^16.11.6",
		"@types/underscore": "^1.11.15",
		"@types/uuid": "^9.0.8",
		"@types/voca": "^1.4.5",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"prettier": "3.2.5",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@observablehq/plot": "^0.6.14",
		"@observablehq/stdlib": "^5.8.6",
		"chart.js": "^4.4.2",
		"chroma-js": "^2.1.2",
		"echarts": "^5.5.0",
		"echarts-gl": "^2.0.9",
		"eslint": "^8.57.0",
		"htl": "^0.3.1",
		"html2canvas": "^1.4.1",
		"openai": "^4.37.1",
		"parse-markdown-table": "^0.0.6",
		"require-text": "^0.0.1",
		"timeline-arrows": "^4.6.0",
		"underscore": "^1.13.6",
		"uuid": "^8.3.2",
		"vis-network": "^9.1.9",
		"vis-timeline": "^7.7.3",
		"voca": "^1.4.1"
	}
}
